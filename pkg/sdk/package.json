{
  "name": "@elitra/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for interacting with Elitra Vaults",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "build:js": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --format=esm",
    "build": "bun run check-types && tsc --emitDeclarationOnly && bun run build:js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint ."
  },
  "keywords": [
    "elitra",
    "vault",
    "defi",
    "erc4626",
    "viem",
    "typescript"
  ],
  "author": "Elitra",
  "license": "MIT",
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.27.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "viem": "^2.0.0"
  }
}
